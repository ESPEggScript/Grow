corner(WalkSlider,8)
stroke(WalkSlider,1,ACCENT,0.18)
ApplyGradient(WalkSlider,Color3.fromRGB(0,255,120),Color3.fromRGB(0,180,220))

WalkSlider.MouseButton1Click:Connect(function()
    Hum.WalkSpeed=200
    notify("WalkSpeed set to 200")
end)

