end)

-------------------------------
-- PAGE 3: Utilities
-------------------------------
local FPSButton=Instance.new("TextButton",Page3)
FPSButton.Size=UDim2.new(0,200,0,36)
FPSButton.Position=UDim2.new(0,16,0,16)
FPSButton.BackgroundColor3=ACCENT_DARK
FPSButton.Text="FPS Boost"
FPSButton.Font=Enum.Font.GothamBold
FPSButton.TextColor3=TEXT
FPSButton.TextSize=14
corner(FPSButton,8)
stroke(FPSButton,1,ACCENT,0.18)
ApplyGradient(FPSButton,Color3.fromRGB(0,255,120),Color3.fromRGB(0,180,220))

FPSButton.MouseButton1Click:Connect(function()
    for i,v in pairs(game:GetService("Lighting"):GetChildren()) do
        if v:IsA("BlurEffect") or v:IsA("SunRaysEffect") or v:IsA("BloomEffect") then v.Enabled=false end
    end
    workspace.DescendantAdded:Connect(function(d)
        if d:IsA("ParticleEmitter") then d:Destroy() end
    end)
    notify("FPS Boost Applied")
end)

local NoclipButton=Instance.new("TextButton",Page3)
NoclipButton.Size=UDim2.new(0,200,0,36)
NoclipButton.Position=UDim2.new(0,16,0,64)
NoclipButton.BackgroundColor3=ACCENT_DARK
NoclipButton.Text="Noclip (Toggle)"
NoclipButton.Font=Enum.Font.GothamBold
NoclipButton.TextColor3=TEXT
NoclipButton.TextSize=14
corner(NoclipButton,8)
stroke(NoclipButton,1,ACCENT,0.18)
ApplyGradient(NoclipButton,Color3.fromRGB(0,255,120),Color3.fromRGB(0,180,220))

local NoclipActive=false
NoclipButton.MouseButton1Click:Connect(function()
    NoclipActive=not NoclipActive
    if NoclipActive then
        Hum:ChangeState(Enum.HumanoidStateType.Physics)
        notify("Noclip Enabled")
    else
        Hum:ChangeState(Enum.HumanoidStateType.RunningNoPhysics)
        notify("Noclip Disabled")
    end
end)

