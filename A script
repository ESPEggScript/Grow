cred.BackgroundTransparency=1
cred.Font=Enum.Font.Gotham
cred.TextSize=14
cred.TextColor3=Color3.fromRGB(200,230,200)
cred.TextWrapped=true
cred.Text="GXScript Hub\n\nUI & Tools\n\nMade by -GXScript"
